{
	"name": "mjs-frontend",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"deploy": "pnpm run build:css && npx wrangler deploy",
		"dev:css": "npx @tailwindcss/cli -i ./public/styles.css -o ./public/dist/styles.css --watch",
		"build:css": "npx @tailwindcss/cli -i ./public/styles.css -o ./public/dist/styles.css --minify",
		"dev": "wrangler dev"
	},
	"devDependencies": {
		"@tailwindcss/cli": "^4.0.0",
		"tailwindcss": "^4.0.0",
		"wrangler": "^4.35.0"
	},
	"dependencies": {
		"hono": "^4.9.7"
	}
}
